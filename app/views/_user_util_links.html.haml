%ul
- if current_user
	%li
		= blink_to "Log Out", logout_path 
		[
		= link_to current_user.login, user_path
		]
  	
	- if current_user.last_search_url
		%li
			= link_to('View your last search', catalog_index_path(current_user.last_search_url))
				
	%li= link_to 'Your Bookmarks', bookmarks_path
	%li= link_to "Saved Searches", saved_searches_path
- else 
	%li= link_to 'Login', login_path

	%li= link_to "Search History", search_history_index_path
